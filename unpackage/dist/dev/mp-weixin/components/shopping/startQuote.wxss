.uni-banner {
  width: 750rpx;
  height: 560rpx;
  position: fixed;
  bottom: 0;
  z-index: 99;
  background: #FFFFFF;
}
.uni-mask {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 50;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
}
