 flex"><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="btn_left" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['sumbit',['$event']]]]]}}" class="btn_right" bindtap="__e">确定</view></view></view>