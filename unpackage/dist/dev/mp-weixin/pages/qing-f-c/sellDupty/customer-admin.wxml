<view><view class="search_top_box"><view class="flex_sb"><view class="flex_c search_left"><icon style="height:14px;margin-left:40rpx;" type="search" size="14"></icon><input class="search_left_input" placeholder="请输入搜索内容" data-event-opts="{{[['input',[['blurInput',['$event']]]]]}}" value="{{inputValueOne}}" bindinput="__e"/></view><button data-event-opts="{{[['tap',[['tapSearch',['$event']]]]]}}" class="searcb_right_btn" bindtap="__e">搜索</button></view></view><view><block wx:for="{{customerList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="list flex_c box_shadow" data-id="{{item.companyCode}}" data-index="{{index}}" data-event-opts="{{[['tap',[['toClientDetail',['$event']]]]]}}" catchtap="__e"><view class="{{[compileing?'wid_610':'wid_670']}}"><view class="flex_sb mt_10"><view class="flex"><image class="title_img" src="/static/images/qingfc/application/companyx.png" mode="aspectFit"></image><view class="fs_16 font_we_bold wid_510">{{item.companyName||''}}</view></view><view class="{{[item.type==2?'id_btn':'seller_btn']}}">{{item.type==2?'买家':'卖家'}}</view></view><view class="flex_c mt_20"><image class="title_img" src="/static/images/qingfc/application/list.png" mode="aspectFit"></image><view class="fs_14 "><text class="mr_60">{{item.regionName||''}}</text><text class="mr_60">{{item.companyTypeName||''}}</text><text>{{(item.linkmanCount||0)+"个联系人"}}</text></view></view><view class="flex_c mt_20"><image class="title_img" src="/static/images/qingfc/application/contacts.png" mode="aspectFit"></image><view class="fs_14">{{(item.deputyRealName||'')+" "+(item.deputyPhone||"")}}</view></view></view></view></block></block><block wx:if="{{loading}}"><view class="bottom_title fff_50">加载中...</view></block></view><view data-event-opts="{{[['tap',[['goCustomerCreated',['$event']]]]]}}" class="fixed_right_bottom box_shadow" bindtap="__e"><view>新建</view><view>客户</view></view></view>