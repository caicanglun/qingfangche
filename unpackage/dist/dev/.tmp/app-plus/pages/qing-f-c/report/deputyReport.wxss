page {
  background: #F4F4F4;
  font-size: 15px;
}
.progessClass {
  height: 20rpx;
  background: -webkit-linear-gradient(4deg, #ffa24f 0%, #ff6000 100%);
  background: linear-gradient(86deg, #ffa24f 0%, #ff6000 100%);
  border-radius: 10rpx;
}
.progressPercent {
  width: 80rpx;
  height: 32rpx;
  background: #ff6600;
  border-radius: 14rpx;
  color: white;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.spanFont {
  font-size: 13px;
}
.numberFont {
  font-size: 16px;
  font-weight: bold;
}
.inTabbar {
  width: 100%;
  background-color: #FFFFFF;
  z-index: 99;
  position: fixed;
  height: 88rpx;
  top: 0;
}
.inTabbar .tabItem {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-weight: 500;
  height: 88rpx;
}
.inTabbar .tabItem .tabText {
  color: black;
}
.active {
  color: #FF6000;
  border-bottom: 4rpx solid #FF6000;
}
.active2 {
  color: #FF6000;
  border-bottom: 2rpx solid #FF6000;
}
.tabItem2 {
  font-size: 14px;
  margin-right: 30rpx;
}
.tabItemRegion {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 14px;
  height: 50rpx;
  padding: 0 10rpx;
  border-radius: 10rpx;
  background: #F4F4F4;
  color: black;
  margin-right: 30rpx;
}
.activeRegion {
  color: white;
  background: #FF6000;
}
.blockWrap {
  padding: 0 30rpx;
}
.box-shawn {
  width: 750rpx;
  background: #FFFFFF;
  -webkit-box-shadow: 0px 4px 8px 0px rgba(0, 0, 0, 0.05);
          box-shadow: 0px 4px 8px 0px rgba(0, 0, 0, 0.05);
}
.line {
  width: 6rpx;
  height: 28rpx;
  background: #ff6600;
  border-radius: 3rpx;
}
.title {
  padding-left: 20rpx;
  font-size: 16px;
  font-weight: bold;
  color: black;
  line-height: 24rpx;
}
.labelTitle {
  font-size: 15px;
  color: #707070;
}
.menu {
  width: 30rpx;
  height: 3rpx;
  border-top: 3rpx solid #595959;
  border-bottom: 3rpx solid #595959;
  background-color: #595959;
  padding: 10rpx 0;
  margin-left: 10rpx;
  background-clip: content-box;
}
.uni-swiper-tab {
  width: 100%;
  overflow: hidden;
  margin-top: 20rpx;
}
.swiper-tab-list {
  font-size: 30rpx;
  width: 150rpx;
  display: inline-block;
  text-align: center;
  color: #555;
}
.uni-tab-bar .active {
  color: #007AFF;
}
.uni-tab-bar .swiper-box {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  width: 100%;
  height: calc(100% - 100rpx);
}
.uni-tab-bar-loading {
  padding: 20rpx 0;
}

