<view><view style="display:flex;flex-direction:column;"><view class="bingpai"><view class="flex_c" style="height:50rpx;width:100%;"><text>织机状态：</text><text>{{productInfo.machineStatusName||''}}</text></view><view class="flex_c" style="height:50rpx;width:100%;"><text class="item">织机数量：</text><block wx:if="{{productInfo.machineCount}}"><text>{{(productInfo.machineCount||'')+"台"}}</text></block></view></view><view class="bingpai"><view class="flex_c" style="height:50rpx;width:100%;"><text class="item">织机类型：</text><text>{{productInfo.machineTypeName||''}}</text></view><view class="flex_c" style="height:50rpx;width:100%;"><text class="item">单机产量：</text><block wx:if="{{productInfo.dailyProduction}}"><text>{{(productInfo.dailyProduction||'')+"米"}}</text></block></view></view><view class="bingpai"><view class="flex_c" style="height:50rpx;width:100%;"><text class="item">织机品牌：</text><text>{{productInfo.machineBrandName||''}}</text></view><view class="flex_c" style="height:50rpx;width:100%;"><text class="item">日产总量：</text><block wx:if="{{productInfo.machineCount*productInfo.dailyProduction}}"><text>{{(productInfo.machineCount*productInfo.dailyProduction||'')+"米"}}</text></block></view></view><view class="bingpai"><view class="flex_c" style="height:50rpx;width:100%;"><text class="item">现货状态：</text><text>{{productInfo.inStockName||''}}</text></view><view class="flex_c" style="height:50rpx;width:100%;"><text class="item">现货数量：</text><block wx:if="{{productInfo.goodsCount}}"><text>{{(productInfo.goodsCount||'')+"米"}}</text></block></view></view></view></view>