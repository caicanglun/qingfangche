.search_btn {
  width: 210rpx;
  /* background-color: #F2F2F2; */
  background-color: #F2F2F2;
  border-radius: 28rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  /* color:#9B9B9B; */
  color: #333;
  height: 56rpx;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  position: relative;
  z-index: 99;
  overflow: hidden;
  -webkit-transition: 0.2s;
  transition: 0.2s;
}
